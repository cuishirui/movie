<h1>edit movie</h1>
<div class="container">
  <%= simple_form_for @movie do |f|%>
    <%= f.input :title%>
    <%= f.input :description%>
    <% if @movie.image.present?%>
      <%= image_tag(@movie.image.thumb.url)%>
    <% end %>
    <%= f.input :image, as: :file%>
    <%= f.submit "submit",data: { dimiss_with: "submitting..."}%>
  <% end %>
</div>
